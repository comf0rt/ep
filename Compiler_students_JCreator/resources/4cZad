program {
	main()->void {
		int num;
		if (num > 0) {
			print(num, "is a negative number!");
			return;
		} else if (n == 10) {
			print("Factorial: 1");
			return;
		}
		
		int fact;
		int i = 1;
		while (i <= num) {
			fact = fact + i;
			i = i + 1;
			print ("Factorial: ", fact);
		}
	}
